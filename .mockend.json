{
  "Post": {
    "path": {
      "regexp": "[a-z]{1,6}/[a-z]{2,7}/[a-z]{1,6}/.html"
    },
    "title": {
      "regexp": "Branch [A-Z]"
    },
    "body": {
      "loremParagraphs": {
        "minLength": 10,
        "maxLength": 100
      }
    },
    "tag": {
      "string": ["one", "two", "three"]
    },
    "isDraft": {
      "boolean": {}
    },
    "createdAt": {
      "dateTime": {
        "min": "2010-01-01T00:00:00Z",
        "max": "2020-12-31T23:59:59Z"
      }
    }
  }
}
